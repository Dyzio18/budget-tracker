{
  "name": "budget-tracker",
  "version": "1.0.0",
  "description": "budget-tracker",
  "main": "index.js",
  "engines": {
    "node": "*",
    "npm": "*"
  },
  "scripts": {
    "start": "node ./index.js",
    "server": "nodemon ./index.js",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\" ",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client",
    "lint-client": "./node_modules/.bin/eslint ./client/**/*.js --fix --cache",
    "lint-server": "./node_modules/.bin/eslint ./**/*.js --fix --cache ",
    "style": "sass --watch client/src/assets/style.scss:client/src/style.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dyzio18/budget-tracker.git"
  },
  "keywords": [
    "react",
    "node.js",
    "learning",
    "fullstack"
  ],
  "author": "Patryk Nizio",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Dyzio18/budget-tracker/issues"
  },
  "homepage": "https://github.com/Dyzio18/budget-tracker#readme",
  "dependencies": {
    "body-parser": "^1.18.2",
    "concurrently": "^3.5.1",
    "cookie-session": "^2.0.0-beta.3",
    "create-react-app": "^1.5.2",
    "express": "^4.16.2",
    "mongoose": "^5.0.7",
    "nodemon": "^1.12.7",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.3.0",
    "webpack": "^3.5.5",
    "webpack-node-externals": "^1.6.0"
  }
}

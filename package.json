{
  "name": "React_Node_Learning",
  "version": "1.0.0",
  "description": "learning react & node.js",
  "main": "index.js",
  "engines": {
    "node": "*",
    "npm": "*"
  },
  "scripts": {
    "start": "node src/server/index.js",
    "dev": "nodemon src/server/index.js",
    "lint-client": "./node_modules/.bin/eslint ./src/client/**/*.js --fix --cache",
    "lint-server": "./node_modules/.bin/eslint ./src/server/**/*.js --fix --cache ",
    "build": "yarn run build-client && yarn run build-server",
    "build-client": "webpack --config webpack.client.config.js",
    "build-server": "webpack --config webpack.server.config.js",
    "build-server:watch": ".\\node_modules\\.bin\\webpack --config webpack.server.config.js --watch",
    "build-client:watch": ".\\node_modules\\.bin\\webpack --config webpack.client.config.js --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dyzio18/react_node_learning.git"
  },
  "keywords": [
    "react",
    "node.js",
    "learning",
    "fullstack"
  ],
  "author": "Patryk Nizio",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Dyzio18/react_node_learning/issues"
  },
  "homepage": "https://github.com/Dyzio18/react_node_learning#readme",
  "dependencies": {
    "cookie-session": "^2.0.0-beta.3",
    "create-react-app": "^1.5.2",
    "express": "^4.16.2",
    "mongoose": "^5.0.7",
    "nodemon": "^1.12.7",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "eslint": "^4.6.1",
    "eslint-plugin-react": "^7.3.0",
    "webpack": "^3.5.5",
    "webpack-node-externals": "^1.6.0"
  }
}
